<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.financeiro.controle.admin.controlefinanceiro.activity.ActivityContacts">

    <Button
        android:text="Voltar"
        android:textSize="20dp"
        android:id="@+id/voltarIDfb"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:textColor="@color/colorPrimaryDark"
        android:background="#ffffff"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Contatos"
        android:id="@+id/textView8"
        android:layout_marginBottom="32dp"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp"
        android:layout_alignBottom="@+id/imageView4"
        android:layout_toRightOf="@+id/imageView4"
        android:layout_toEndOf="@+id/imageView4" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageView4"
        android:background="@drawable/logo"
        android:layout_marginTop="26dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/voltarIDfb"
        android:layout_alignStart="@+id/voltarIDfb"
        android:layout_marginLeft="29dp"
        android:layout_marginStart="29dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Nome:"
        android:id="@+id/textView7"
        android:layout_marginTop="62dp"
        android:paddingLeft="10dp"
        android:layout_below="@+id/imageView4"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:textSize="14dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Airton Siqueira de Freitas"
        android:id="@+id/textView9"
        android:layout_alignBottom="@+id/textView7"
        android:layout_toEndOf="@+id/textView7"
        android:layout_toRightOf="@+id/textView7"
        android:paddingLeft="10dp"
        android:textSize="14dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="E-mail:"
        android:id="@+id/textView10"
        android:layout_below="@+id/textView7"
        android:layout_alignLeft="@+id/voltarIDfb"
        android:layout_alignStart="@+id/voltarIDfb"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:textSize="14dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/emailAirton"
        android:id="@+id/textEmail"
        android:textSize="12dp"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:layout_alignBottom="@+id/textView10"
        android:layout_toRightOf="@+id/textView10"
        android:layout_toEndOf="@+id/textView10" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:autoLink="web"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/LinkedAirton"
        android:id="@+id/textLinked"
        android:textSize="12dp"
        android:layout_alignBottom="@+id/textView17"
        android:layout_toRightOf="@+id/textView17"
        android:layout_toEndOf="@+id/textView17"
        android:paddingLeft="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Link:"
        android:id="@+id/textView17"
        android:layout_below="@+id/textView10"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:layout_alignRight="@+id/textView10"
        android:layout_alignEnd="@+id/textView10"
        android:textSize="14dp" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView5"
        android:background="@drawable/airton"
        android:layout_alignTop="@+id/textView9"
        android:layout_alignRight="@+id/voltarIDfb"
        android:layout_alignEnd="@+id/voltarIDfb"
        android:layout_marginRight="20dp"
        android:layout_marginEnd="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Nome:"
        android:id="@+id/textView12"
        android:layout_marginTop="69dp"
        android:paddingLeft="10dp"
        android:textSize="14dp"
        android:layout_below="@+id/textLinked"
        android:layout_alignLeft="@+id/textView17"
        android:layout_alignStart="@+id/textView17" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Rafael Siqueira de Freitas"
        android:id="@+id/textView13"
        android:paddingLeft="10dp"
        android:textSize="14dp"
        android:layout_alignBottom="@+id/textView12"
        android:layout_toRightOf="@+id/textView17"
        android:layout_toEndOf="@+id/textView17" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="E-mail:"
        android:id="@+id/textView14"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:textSize="14dp"
        android:layout_below="@+id/textView12"
        android:layout_alignLeft="@+id/textView12"
        android:layout_alignStart="@+id/textView12" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/emailFael"
        android:id="@+id/textEmail2"
        android:textSize="12dp"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:layout_alignTop="@+id/textView14"
        android:layout_toRightOf="@+id/textView12"
        android:layout_toEndOf="@+id/textView12" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Link:"
        android:id="@+id/textView18"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:textSize="14dp"
        android:layout_below="@+id/textView14"
        android:layout_alignLeft="@+id/textView14"
        android:layout_alignStart="@+id/textView14"
        android:layout_alignRight="@+id/textView14"
        android:layout_alignEnd="@+id/textView14" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:autoLink="web"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/linkFael"
        android:id="@+id/textLinked2"
        android:textSize="12dp"
        android:paddingLeft="10dp"
        android:layout_alignBottom="@+id/textView18"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@+id/textEmail2"
        android:layout_alignStart="@+id/textEmail2" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView6"
        android:background="@drawable/fael"
        android:layout_above="@+id/textLinked2"
        android:layout_alignLeft="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5" />


</RelativeLayout>
