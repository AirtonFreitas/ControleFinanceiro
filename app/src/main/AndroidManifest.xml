        android:theme="@style/AppTheme">
        <activity android:name="com.financeiro.controle.admin.listadecompras.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.financeiro.controle.admin.listadecompras.activity.MainCadastro" />
        <activity android:name="com.financeiro.controle.admin.listadecompras.activity.LoginActivity" />
        <activity
            android:name="com.financeiro.controle.admin.listadecompras.activity.principalActivity"
